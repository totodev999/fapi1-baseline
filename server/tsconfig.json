{
  "compilerOptions": {
    "target": "ES2021",
    "module": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "$schema": "https://json.schemastore.org/tsconfig"
}
